{
  "name": "PGWireless/php-msf",
  "description": "PGWireless Micro Service Framework For PHP",
  "license": "BSD-3-Clause",
  "type": "library",
  "homepage": "https://github.com/PGWireless/php-msf",
  "require": {
    "php": ">=7.0",
    "PGWireless/php-log": "dev-master",
    "hassankhan/config": "~0.10",
    "voku/anti-xss": "2.0.1",
    "league/plates": "3.*",
    "protobuf-php/protobuf": "~0.1.2",
    "protobuf-php/protobuf-plugin": "^0.1.2",
    "alcaeus/mongo-php-adapter": "^1.0"
  },
  "repositories": {
    "php-log": {
      "type": "vcs",
      "url": "http://192.168.1.45:3000/PGWireless/php-log.git"
    }
  },
  "autoload": {
    "psr-4": {
      "PG\\MSF\\Server\\": "src/Server",
      "PG\\MSF\\Client\\": "src/Client",
      "PG\\MSF\\Test\\": "src/test"
    },
    "files": [
      "src/Server/Helpers/Common.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
  },
  "config": {
    "secure-http": false
  }
}
