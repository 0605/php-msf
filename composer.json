{
  "name": "pinguo/php-msf",
  "description": "pinguo Micro Service Framework For PHP",
  "license": "BSD-3-Clause",
  "type": "library",
  "homepage": "https://github.com/pinguo/php-msf",
  "require": {
    "php": ">=7.0",
    "ext-swoole": ">=1.9.15",
    "pinguo/php-log": "dev-master",
    "pinguo/php-context": "dev-master",
    "pinguo/php-aop": "dev-master",
    "pinguo/php-exception": "dev-master",
    "pinguo/plates": "dev-master",
    "hassankhan/config": "~0.10",
    "alcaeus/mongo-php-adapter": "^1.0",
    "flexihash/flexihash": "^2.0.0"
  },
  "autoload": {
    "psr-4": {
      "PG\\MSF\\": "src/"
    },
    "files": [
      "src/Helpers/Common.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
  },
  "config": {
    "secure-http": false
  }
}
